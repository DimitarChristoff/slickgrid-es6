{
  "name": "slickgrid-es6",
  "version": "2.1.0",
  "description": "Non-tracking fork of 6pac/SlickGrid and X-SlickGrid that makes it easy to consume via npm under webpack/babel and without jquery-ui",
  "main": "dist/slick.es6.min.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --progress --display-optimization-bailout --"
  },
  "author": "Michael Leibman",
  "license": "MIT",
  "dependencies": {
    "flatpickr": "^3.0.5-1",
    "interactjs": "^1.3.0-alpha.1",
    "jquery": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "~6.24.1",
    "babel-core": "~6.25.0",
    "babel-loader": "~6.4.1",
    "babel-polyfill": "~6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "babel-runtime": "~6.23.0",
    "copy-webpack-plugin": "~4.0.1",
    "cross-env": "~5.0.0",
    "css-loader": "~0.28.0",
    "faker": "~4.1.0",
    "fecha": "~2.3.1",
    "file-loader": "~0.11.1",
    "font-awesome": "^4.7.0",
    "history": "~4.6.3",
    "html-loader": "~0.4.5",
    "image-webpack-loader": "~3.3.0",
    "less": "~2.7.2",
    "less-loader": "~4.0.3",
    "numeral": "~2.0.6",
    "react": "~15.6.1",
    "react-dom": "~15.6.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "rimraf": "^2.6.1",
    "style-loader": "~0.16.1",
    "url-loader": "~0.5.8",
    "webpack": "~3.0.0",
    "webpack-dev-server": "~2.5.0",
    "webpack-node-externals": "~1.6.0"
  }
}
